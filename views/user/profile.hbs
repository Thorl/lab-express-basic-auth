{{#if userInSession}}
  <div class="profile-page-container">
    <h2 class="profile-page-header">Welcome to your profile</h2>
    <p class="profile-page-paragraph">Logged in as:
      {{userInSession.username}}</p>
    <a href="/private">Go to the Private Page</a>
    <a href="/main">Go to the Main Page</a>
  </div>
{{else}}
  <p class="not-logged-in-message">Error: User not logged in</p>
{{/if}}